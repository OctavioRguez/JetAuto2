<?xml version="1.0" ?>
<launch>
    <!-- SLAM Package-->
    <node pkg="gmapping" type="slam_gmapping" name="gmapping">
        <param name="base_frame"              value="base_link" />
        <param name="odom_frame"              value="odom" />
        <param name="map_frame"               value="map" />
        <rosparam command="load"              file="$(find jet_auto2)/config/jetautoGmapping.yaml" />
    </node>

    <!-- Nodes -->
    <node name="ObstacleAvoidance" pkg="jet_auto2" type="lidarAvoidance.py" output='screen' />
    <node name="ArmMovement" pkg="jet_auto2" type="armMovement.py" output='screen'/>
</launch>